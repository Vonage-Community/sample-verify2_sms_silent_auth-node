<!doctype html>
<html>
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script src="/tailwind.css"></script>
  <style type="text/tailwindcss">
    @layer utilities {
      .content-auto {
        content-visibility: auto;
      }
      h1 { @apply mt-12 text-4xl font-bold tracking-tight text-gray-900 sm:text-5xl; }
      .button { @apply rounded-md bg-indigo-600 px-4 py-1 text-base font-normal text-white shadow-sm hover:bg-indigo-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600; }
    }
  </style>
</head>
<body class="bg-slate-50">
  <div class="max-w-md mx-auto bg-white px-6 py-12 text-center border border-gray-200 rounded-2xl my-4">
    <a href="/"><img src="/favicon.ico" alt="logo" class="mx-auto h-12 w-auto" /></a>